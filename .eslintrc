{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "max-len": [
      "error",
      300
    ],
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "object-curly-newline": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "function-paren-newline": 0,
    "no-console": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "no-mixed-operators": 0,
    "indent": 0,
    "no-unused-expressions": 0
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolve": {
      "moduleDirectory": [
        "node_modules"
      ]
    }
  },
  "globals": {}
}
